<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Repair Order Sequence -->
        <record id="seq_repair_order" model="ir.sequence">
            <field name="name">Repair Order</field>
            <field name="code">repair.order</field>
            <field name="prefix">RMA/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>

          <!-- Maintenance Request Sequence -->
        <record id="seq_maintenance_req" model="ir.sequence">
            <field name="name">Maintenance Request</field>
            <field name="code">maintenance.request</field>
            <field name="prefix">MR/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
	</data>

</odoo>
