<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="view_product_supplier_inherit_tf_peec_purchase" model="ir.ui.view">
            <field name="name">product.template.supplier.form.inherit.tf.peec.purchase</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="purchase.view_product_supplier_inherit"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='bill']" position="before">
                    <group string="Cement Plants" colspan="3">
                        <field name="cement_plant_ids" nolabel="1"/>
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</odoo>