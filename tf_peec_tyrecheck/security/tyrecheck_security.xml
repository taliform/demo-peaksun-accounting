<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="module_category_tyrecheck" model="ir.module.category">
            <field name="name">TyreCheck</field>
        </record>

        <record id="group_tyrecheck_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_tyrecheck"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>

    </data>
</odoo>
