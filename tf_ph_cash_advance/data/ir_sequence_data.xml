<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
    	<!-- Cash Advance sequence -->
        <record id="seq_cash_advance" model="ir.sequence">
            <field name="name">Cash Advance</field>
            <field name="code">cash.advance</field>
            <field name="prefix">CA/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Direct Reimbursement sequence -->
        <record id="seq_direct_reimbursement" model="ir.sequence">
            <field name="name">Direct Reimbursement</field>
            <field name="code">direct.reimbursement</field>
            <field name="prefix">DR/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- CA Liquidation sequence -->
        <record id="seq_cash_advance_transaction" model="ir.sequence">
            <field name="name">Cash Advance Transaction</field>
            <field name="code">cash.advance.transaction</field>
            <field name="prefix">CAT/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- DR Liquidation sequence -->
        <record id="seq_direct_reimbursement_transaction" model="ir.sequence">
            <field name="name">Direct Reimbursement Transaction</field>
            <field name="code">direct.reimbursement.transaction</field>
            <field name="prefix">DRT/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>

	</data>
</odoo>